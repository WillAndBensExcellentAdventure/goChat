{
  "name": "goChag",
  "description": "real time chat client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "mocha --recursive tests --exit",
    "start": "node src/server/index.js",
    "build": "webpack --mode production",
    "client": "NODE_ENV=development webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "NODE_ENV=development nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "docs": "rm -rf docs; jsdoc -r src -d docs -a all; python -mwebbrowser docs/index.html"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "add-module-exports",
      "syntax-dynamic-import",
      "transform-runtime"
    ],
    "retainLines": true
  },
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^1.1.1",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "es6-promise": "^4.2.4",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "postcss-loader": "^2.1.5",
    "autoprefixer": "^8.5.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "5.0.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.17.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.20.3",
    "supertest": "3.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "unit.js": "2.0.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
